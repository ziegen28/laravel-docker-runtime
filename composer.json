{
  "name": "ziegen2801/laravel-docker-runtime",
  "description": "Sail++ Docker Runtime for Laravel (Apache-first, UID/GID safe)",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.2|^8.3|^8.4",
    "illuminate/support": "^10|^11|^12"
  },
  "autoload": {
    "psr-4": {
      "Ziegen2801\\DockerRuntime\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Ziegen2801\\DockerRuntime\\DockerRuntimeServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
